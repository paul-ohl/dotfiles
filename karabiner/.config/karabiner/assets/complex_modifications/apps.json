{
  "title": "Launch apps",
  "rules": [
    {
      "description": "Launch apps with hyper+letters.",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_control",
                "left_shift",
                "left_option"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Brave Browser.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_control",
                "left_shift",
                "left_option"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/kitty"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_control",
                "left_shift",
                "left_option"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/kitty"
            }
          ]
        }
      ]
    }
  ]
}
